
public class exampleone  

          {

    public static void main (String[] args) {
        
    int a=23;
    int b=4;
     int c=5;

    //Addition

    int sum = a+b+c;

    //subtraction
    int sub=a-b-c;

    //multiplication
    int pro=a*b*c;

    //division
    double div=a/b/c;


System.out.println("The sum  of a,b,c is :" + sum);

 System.out.println("The sub of a,b,c is :" + sub);       

System.out.println("The product of a,b,c is :" + pro);

System.out.println("The division of a,b,c is :" + div);

}
    }
